{
  "useTabs": true,
  "tabWidth": 4,
  "endOfLine": "lf",
  "printWidth": 80,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "semi": false,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleQuote": true,
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
